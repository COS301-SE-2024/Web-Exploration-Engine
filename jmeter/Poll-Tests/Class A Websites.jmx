          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor - Save Scrape results">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonOutput

// Specify the folder and file name for the text file
def textFilePath = &apos;/home/ndlovu/GitHub/Web-Exploration-Engine/jmeter/Load-Tests/results/response_&apos; + System.currentTimeMillis() + &apos;.txt&apos;

// Specify the folder and file name for the JSON file
def jsonFilePath = &apos;/home/ndlovu/GitHub/Web-Exploration-Engine/jmeter/Load-Tests/results/json_response_&apos; + System.currentTimeMillis() + &apos;.json&apos;

// Log file paths for debugging
log.info(&apos;Text file path: &apos; + textFilePath)
log.info(&apos;JSON file path: &apos; + jsonFilePath)

// Open the text file for writing
def textFile = new File(textFilePath)
textFile.withWriter { writer -&gt;
    writer &lt;&lt; &apos;Response Code: &apos; + prev.getResponseCode() + &apos;\n&apos;
    writer &lt;&lt; &apos;Response Message: &apos; + prev.getResponseMessage() + &apos;\n&apos;
    writer &lt;&lt; &apos;Response Body: &apos; + prev.getResponseDataAsString() + &apos;\n&apos;
    writer &lt;&lt; &apos;\n--- Request Details ---\n&apos;
    writer &lt;&lt; &apos;Request URL: &apos; + sampler.getUrl() + &apos;\n&apos;
    writer &lt;&lt; &apos;Request Method: &apos; + sampler.getMethod() + &apos;\n&apos;
    writer &lt;&lt; &apos;Headers:\n&apos;
    sampler.getHeaderManager().getHeaders().each { header -&gt;
        writer &lt;&lt; header.getName() + &apos;: &apos; + header.getValue() + &apos;\n&apos;
    }
    writer &lt;&lt; &apos;Request Body: &apos; + sampler.getQueryString() + &apos;\n&apos;
}

// Create a JSON object for the JSON file
def jsonResponse = [
    responseCode   : prev.getResponseCode(),
    responseMessage: prev.getResponseMessage(),
    responseBody   : prev.getResponseDataAsString(),
    requestDetails : [
        url    : sampler.getUrl(),
        method : sampler.getMethod(),
        headers: sampler.getHeaderManager().getHeaders().collectEntries { header -&gt;
            [header.getName(), header.getValue()]
        },
        body   : sampler.getQueryString()
    ]
]

// Open the JSON file for writing
def jsonFile = new File(jsonFilePath)
jsonFile.withWriter { writer -&gt;
    writer &lt;&lt; &apos;Response Code: &apos; + prev.getResponseCode() + &apos;\n&apos;
    writer.write(JsonOutput.prettyPrint(JsonOutput.toJson(jsonResponse)))
}

// Log completion
log.info(&apos;Files saved successfully.&apos;)
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PostProcessor>
