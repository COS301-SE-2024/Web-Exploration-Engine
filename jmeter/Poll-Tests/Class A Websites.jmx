          <stringProp name="script">import groovy.json.JsonSlurper

// Define the file path where you want to save the results
def resultsFilePath = &apos;/home/ndlovu/GitHub/Web-Exploration-Engine/jmeter/Poll-Tests/results/polling_results_&apos; + System.currentTimeMillis() + &apos;.txt&apos;

// Open the file for writing
def resultsFile = new File(resultsFilePath)

// Gather required information
def startTime = vars.get(&apos;startTime&apos;)
def pollingURL = &quot;http://localhost:3002/api/scraper/status?url=https://toscrape.com/&amp;type=scrape&quot;

// Initialize pollCount, check if it exists
def totalPolls = vars.get(&apos;pollCount&apos;) ? Integer.parseInt(vars.get(&apos;pollCount&apos;)) : 0 // Default to 0 if not set
def pollingInterval = vars.get(&apos;pollingInterval&apos;)
def endTime = System.currentTimeMillis()
def timedOut = (endTime - Long.valueOf(startTime) &gt;= Long.valueOf(vars.get(&apos;timeout&apos;)))

// Write results to file
resultsFile &lt;&lt; &quot;Polling Results:\n&quot;
resultsFile &lt;&lt; &quot;====================\n&quot;
resultsFile &lt;&lt; &quot;Start Time: &quot; + new Date(Long.valueOf(startTime)).format(&quot;yyyy-MM-dd HH:mm:ss&quot;) + &quot;\n&quot;
resultsFile &lt;&lt; &quot;Polling URL: &quot; + pollingURL + &quot;\n&quot;
resultsFile &lt;&lt; &quot;Total Polls: &quot; + totalPolls + &quot;\n&quot;
resultsFile &lt;&lt; &quot;Polling Interval: &quot; + pollingInterval + &quot; ms\n&quot;
resultsFile &lt;&lt; &quot;Timed Out: &quot; + timedOut + &quot;\n&quot;
resultsFile &lt;&lt; &quot;End Time: &quot; + new Date(endTime).format(&quot;yyyy-MM-dd HH:mm:ss&quot;) + &quot;\n&quot;

// Close the file
resultsFile.close()

// Optional: Log for debugging
log.info(&quot;Polling results saved to: &quot; + resultsFilePath)
</stringProp>
